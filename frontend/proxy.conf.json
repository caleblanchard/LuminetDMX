[
  {
    "context": ["/api/**"],
    "target": "http://localhost:3000",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug",
    "headers": {
      "Connection": "keep-alive"
    }
  },
  {
    "context": ["/ws"],
    "target": "http://localhost:3000",
    "secure": false,
    "changeOrigin": true,
    "ws": true,
    "logLevel": "debug"
  }
]